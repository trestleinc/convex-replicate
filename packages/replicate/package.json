{
  "name": "@trestleinc/replicate",
  "version": "0.3.0",
  "description": "Offline-first data replication with Yjs CRDTs and Convex",
  "repository": "github:trestleinc/convex-replicate",
  "homepage": "https://github.com/trestleinc/convex-replicate#readme",
  "bugs": {
    "url": "https://github.com/trestleinc/convex-replicate/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "convex",
    "replicate",
    "crdt",
    "yjs",
    "offline-first",
    "tanstack"
  ],
  "type": "module",
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": {
        "@convex-dev/component-source": "./src/client/index.ts",
        "types": "./dist/client/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server.js"
      }
    },
    "./ssr": {
      "import": {
        "types": "./dist/server/ssr.d.ts",
        "default": "./dist/ssr.js"
      }
    },
    "./convex.config": {
      "import": {
        "@convex-dev/component-source": "./src/component/convex.config.ts",
        "types": "./dist/component/convex.config.d.ts",
        "default": "./dist/component/convex.config.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "yjs": "13.6.27",
    "@tanstack/offline-transactions": "0.1.2",
    "@logtape/logtape": "1.1.2"
  },
  "peerDependencies": {
    "convex": "1.28.2",
    "@tanstack/db": "0.4.19"
  },
  "devDependencies": {
    "@rslib/core": "0.17.1"
  },
  "main": "./dist/index.js",
  "types": "./dist/client/index.d.ts",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  }
}
