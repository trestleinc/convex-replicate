{
  "name": "@trestleinc/replicate",
  "version": "1.0.0",
  "description": "Offline-first data replication with Yjs CRDTs and Convex",
  "repository": "github:trestleinc/replicate",
  "homepage": "https://github.com/trestleinc/replicate#readme",
  "bugs": {
    "url": "https://github.com/trestleinc/replicate/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "convex",
    "replicate",
    "crdt",
    "yjs",
    "offline-first",
    "tanstack"
  ],
  "type": "module",
  "private": false,
  "resolutions": {
    "convex": "1.28.2"
  },
  "overrides": {
    "convex": "1.28.2"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "dev:example": "cd examples/tanstack-start && pnpm run dev",
    "build:example": "cd examples/tanstack-start && pnpm run build",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "vitest run && vitest run --config vitest.browser.config.ts",
    "test:watch": "vitest",
    "test:bench": "vitest bench",
    "test:coverage": "vitest run --coverage",
    "test:convex": "cd src/test && npx convex dev",
    "prepublish": "pnpm run build && pnpm run check:fix"
  },
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": {
        "@convex-dev/component-source": "./src/client/index.ts",
        "types": "./dist/client/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server.js"
      }
    },
    "./convex.config": {
      "import": {
        "@convex-dev/component-source": "./src/component/convex.config.ts",
        "types": "./dist/component/convex.config.d.ts",
        "default": "./dist/component/convex.config.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@effect/platform": "^0.93.3",
    "@logtape/logtape": "1.1.2",
    "@tanstack/offline-transactions": "0.1.3",
    "effect": "^3.19.6",
    "idb-keyval": "^6.2.2",
    "y-indexeddb": "^9.0.12",
    "yjs": "13.6.27"
  },
  "peerDependencies": {
    "@tanstack/db": "0.4.20",
    "convex": "1.28.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@effect/vitest": "^0.27.0",
    "@rslib/core": "0.17.1",
    "@tanstack/db": "0.4.20",
    "@types/node": "24.10.0",
    "@vitest/browser-playwright": "4.0.8",
    "@vitest/coverage-v8": "4.0.8",
    "@vitest/ui": "^4.0.8",
    "concurrently": "^9.2.1",
    "convex": "1.28.2",
    "dotenv": "^17.2.3",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^27.1.0",
    "playwright": "^1.52.0",
    "typescript": "5.9.3",
    "vitest": "4.0.8",
    "zod": "4.1.12"
  },
  "main": "./dist/index.js",
  "types": "./dist/client/index.d.ts",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  }
}
